{
    "name": "final-gene/rest-resource-authentication-module",
    "description": "Authentication functionality for REST resources",
    "type": "library",
    "homepage": "https://github.com/final-gene/zf2-rest-resource-authentication-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Giesecke",
            "email": "frank.giesecke@final-gene.de",
            "role": "Developer"
        },
        {
            "name": "Frank Emmrich",
            "email": "kontakt@frank-emmrich.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laminas-api-tools/api-tools-api-problem": "^1.6",
        "laminas-api-tools/api-tools-rest": "^1.8",
        "laminas/laminas-modulemanager": "^2.19",
        "laminas/laminas-stdlib": "^3.21",
        "symfony/console": "^7.3"
    },
    "require-dev": {
        "laminas/laminas-test": "^4.12",
        "phpspec/prophecy-phpunit": "^2.4",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "FinalGene\\RestResourceAuthenticationModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FinalGene\\RestResourceAuthenticationModuleTest\\": "tests/src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "archive": {
        "exclude": [
            "/.*",
            "/composer.*",
            "/phpunit.*",
            "/tests"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true
        },
        "platform": {
            "php": "8.2.29",
            "ext-intl": "0"
        }
    }
}
